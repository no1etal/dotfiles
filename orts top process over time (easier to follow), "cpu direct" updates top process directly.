[32m[m[32m#-------- filter {{{[m
[32m[m[32m#------------------------------------------------------[m
[32m[m[32m# https://newsbeuter.org/doc/newsbeuter.html#_filter_language[m
[32m[m[32m# only works with Attributes > Context > feed,article[m
[32m[m[32m# e.g: feedtitle, description, feedlink, feeddate, rssurl, unread_count, total_count, tags, feedindex[m
[7m[32m [m
[32m[m[32m# 'title' is not available[m
[32m[m[32m# 'author' is not available[m
[7m[32m [m
[32m[m[32m# Feed: gotbletu                                              <--- feedtitle[m
[32m[m[32m# Title: browsh - Modern Text-Based Browser - Linux TUI[m
[32m[m[32m# Author: gotbletu[m
[32m[m[32m# Link: https://www.youtube.com/watch?v=HZq86XfBoRo[m
[32m[m[32m# Date: Wed, 11 Jul 2018 17:00:03 -0700[m
[7m[32m [m
[32m[m[32mdefine-filter "Top 10 Most Pirated Movies of The Week on BitTorrent" "feedtitle =~ \"DVDrip – TorrentFreak\""[m
[32m[m[32mdefine-filter "gotbletu" "feedtitle =~ \"gotbletu\" or description =~ \"gotbletu\""[m
[32m[m[32m# define-filter "windowsdec" "description =~ \"windows\""[m
[32m[m[32m# define-filter "windows" "title = \"firearms\""[m
[32m[m[32m# define-filter "gotbletudesc" "description =~ \"gotbletu\""[m
[32m[m[32m# define-filter "alt living" "feedtitle =~ \"tiny house\""[m
[32m[m[32m# define-filter "Top10TagANDFeedTitle" "tags # \"Video-MovieTalk\" and feedtitle =~ \"Top 10 Most Pirated Movies of The Week on BitTorrent\""[m
[32m[m[32m# define-filter "Top10TagANDFeelink" "tags # \"Video-MovieTalk\" and feedlink =~ \"https://torrentfreak.com/top-10-pirated-movies-week-bittorrent\""[m
[32m[m[32m# define-filter "Top10FeedTitle-Top10" "description =~ \"Top 10 Most Pirated Movies of The Week on BitTorrent\""[m
[7m[32m [m
[32m[m[32m# }}}[m
